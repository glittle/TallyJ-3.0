<link href="@ClientFile("~/Scripts/jquery.qtip.min.css")" rel="stylesheet" type="text/css" />
@if (ViewData["ExtraCss"] != null)
{
  foreach (var s in (string[])ViewData["ExtraCss"])
  {
    <link href="@s" type="text/css" rel="stylesheet" />
  }
}
<link href="@ClientFile("~/Content/Site.css")" rel="stylesheet" type="text/css" />
@AddResourceFilesForViews("css")

